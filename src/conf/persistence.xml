<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="PESIPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>co.edu.unal.pesi.modelo.Entidad</class>
    <class>co.edu.unal.pesi.modelo.Grupos</class>
    <class>co.edu.unal.pesi.modelo.Subgrupos</class>
    <class>co.edu.unal.pesi.modelo.Procesos</class>
    <class>co.edu.unal.pesi.modelo.Procesosorganizaciones</class>
    <class>co.edu.unal.pesi.modelo.Clasedatos</class>
    <class>co.edu.unal.pesi.modelo.Organizaciones</class>
    <class>co.edu.unal.pesi.vista.Servicio</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/sistemainformacion?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.password" value="manager"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
